<h1>Products#show</h1>

  <div class="table-responsive">
    <table class="table">
      <% @products.each do |product| %>
        <tr>
          <td><%= product.name %><td>
          <td><%= product.category %><td>
          <td><%= product.price %><td>
          <td><%= product.stock_quantity %><td>
          <td><%= button_to 'add to cart', cartitems_path, params: {id: product.id}, remote: true %><td>
        </tr> 
      <% end %>
    </table>
  </div>

<div id='test'>
  <% @cartitems.each do |item| %>
  <%= item.product.name %>
  <% end %>
</div>

